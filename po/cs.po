#: src/irctabitem.cpp:2377
msgid "OPER <login> <password>, oper up."
msgstr "OPER <přihlašovací jméno> <heslo>, aktivuje operátora."

#: src/irctabitem.cpp:2382
msgid "PART <channel> [reason], leaves channel."
msgstr "PART <kanál>, opustí kanál."

#: src/irctabitem.cpp:2387
msgid "QUERY <nick>, opens query with nick."
msgstr "QUERY <přezdívka>, otevře záložku pro rozhovor s uživatelem."

#: src/irctabitem.cpp:2392
msgid "QUIT, closes application."
msgstr "QUIT, zavře aplikaci."

#: src/irctabitem.cpp:2397
msgid "QUOTE [text], sends text to server."
msgstr "QUOTE [text], pošle text na server."

#: src/irctabitem.cpp:2402
msgid "SAY [text], sends text to current tab."
msgstr "SAY [text], pošle text do aktuální záložky."

#: src/irctabitem.cpp:2407
msgid ""
"STATS <type>, shows some irc server usage statistics. Available types vary "
"slightly per server; some common ones are:"
msgstr ""
"STATS <typ>, zobrazí některé statistiky irc serveru. Dostupné typy se na "
"jednotlivých serverech mohou lišit; některé z obvyklých:"

#: src/irctabitem.cpp:2408
msgid ""
"c - shows C and N lines for a given server.  These are the names of the "
"servers that are allowed to connect."
msgstr ""
"c - zobrazí C a N spojení pro daný server. Jsou to jména serverů povolených "
"pro připojení."

#: src/irctabitem.cpp:2409
msgid "h - shows H and L lines for a given server (Hubs and Leaves)."
msgstr "h - zobrazí H a L spojení pro daný server."

#: src/irctabitem.cpp:2410
msgid ""
"k - show K lines for a server.  This shows who is not allowed to connect and "
"possibly at what time they are not allowed to connect."
msgstr ""
"k - zobrazí K spojení serveru. Zobrazí nepovolené pro připojení a možná čas "
"po jaký jsou nepovolení."

#: src/irctabitem.cpp:2411
msgid "i - shows I lines. This is who CAN connect to a server."
msgstr "i - zobrazí I spojení. Můžou se připojit k serveru."

#: src/irctabitem.cpp:2412
msgid ""
"l - shows information about amount of information passed to servers and "
"users."
msgstr ""
"l - zobrazí informace o počtu informací poslaných serverům a uživatelům."

#: src/irctabitem.cpp:2413
msgid ""
"m - shows a count for the number of times the various commands have been "
"used since the server was booted."
msgstr "m - zobrazí kolikrát byl různý příkaz použit od startu serveru."

#: src/irctabitem.cpp:2414
msgid "o - shows the list of authorized operators on the server."
msgstr "o - zobrazí seznam autorizovaných operátorů na serveru."

#: src/irctabitem.cpp:2415
msgid "p - shows online operators and their idle times."
msgstr "p - zobrazí online operátory a jejich čas nečinosti."

#: src/irctabitem.cpp:2416
msgid "u - shows the uptime for a server."
msgstr "u - zobrazí uptime serveru."

#: src/irctabitem.cpp:2417
msgid ""
"y - shows Y lines, which lists the various connection classes for a given "
"server."
msgstr ""
"y - zobrazí Y spojení, což je seznam různých tříd připojení pro daný server."

