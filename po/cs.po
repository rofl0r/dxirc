msgid "WHOAMI, whois about you."
msgstr "WHOAMI, whois o tobě."

#: src/irctabitem.cpp:2343
msgid "WHOIS <nick>, whois nick."
msgstr "WHOIS <přezdívka>, příkaz 'kdo je' pro přezdívku."

#: src/irctabitem.cpp:2348
msgid "WHOWAS <nick>, whowas nick."
msgstr "WHOWAS <přezdívka>,  příkaz 'kdo byl' pro přezdívku."

#: src/irctabitem.cpp:2356
msgid "Command is empty, type /commands for available commands"
msgstr "Příkaz je prázdný, /commands zobrazí dostupné příkazy"

