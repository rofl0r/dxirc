#: src/irctabitem.cpp:2234
msgid ""
"ADMIN [server], finds information about administrator for current server or "
"[server]."
msgstr ""
"ADMIN [server], vyhledá informace o administrátorovi aktuálního serveru nebo "
"[serveru]."

#: src/irctabitem.cpp:2239
msgid "AWAY [message], sets away status."
msgstr "AWAY [zpráva], nastaví stav pryč."

#: src/irctabitem.cpp:2244
msgid "BANLIST <channel>, shows banlist for channel."
msgstr "BANLIST <kanál>, zobrazí seznam banů kanálu."

#: src/irctabitem.cpp:2249
msgid ""
"CONNECT <server> [port] [nick] [password] [realname] [channels], connects "
"for given server."
msgstr ""
"CONNECT <server> [port] [přezdívka] [heslo] [jméno] [kanály], připojí k "
"danému serveru."

#: src/irctabitem.cpp:2254
msgid "CTCP <nick> <message>, sends a CTCP message to a user."
msgstr "CTCP <přezdívka> <zpráva>, pošle CTCP zprávu uživateli."

#: src/irctabitem.cpp:2259
msgid "CYCLE <channel> [message], leaves and join channel."
msgstr "CYCLE <kanál> [zpráva], opustí a připojí kanál."

#: src/irctabitem.cpp:2264
msgid "DCC chat <nick>, starts DCC chat."
msgstr "DCC chat <přezdívka>, spustí DCC chat."

#: src/irctabitem.cpp:2265
msgid "DCC send <nick> <filename>, sends file over DCC."
msgstr "DCC send <přezdívka> <soubor>, pošle soubor pomocí DCC."

#: src/irctabitem.cpp:2266
msgid "DCC psend <nick> <filename>, sends file passive over DCC."
msgstr "DCC psend <přezdívka> <soubor>, pošle soubor pasivně pomocí DCC."

#: src/irctabitem.cpp:2267
msgid ""
"More information about passive DCC on http://en.wikipedia.org/wiki/"
"Direct_Client-to-Client#Passive_DCC"
msgstr ""
"Více informací o pasivním DCC na http://en.wikipedia.org/wiki/Direct_Client-"
"to-Client#Passive_DCC"

#: src/irctabitem.cpp:2272
msgid "DEOP <channel> <nicks>, removes operator status from one or more nicks."
msgstr ""
"DEOP <kanál> <přezdívky>, odebere operátora jedné nebo více přezdívkám."

#: src/irctabitem.cpp:2277
msgid "DEVOICE <channel> <nicks>, removes voice from one or more nicks."
msgstr "DEVOICE <kanál> <přezdívky>, odebere hlas jedné nebo více přezdívkám."

#: src/irctabitem.cpp:2282
msgid "DISCONNECT [reason], leaves server."
msgstr "DISCONNECT [důvod], opustí server."

#: src/irctabitem.cpp:2288
msgid ""
"EXEC [-o|-c] <command>, executes command, -o sends output to channel/query, -"
"c closes running command."
msgstr ""
"EXEC [-o|-c] <příkaz>, spustí  příkaz, -o pošle výstup do kanálu/rozhovoru, -"
"c zavře běžící příkaz."

#: src/irctabitem.cpp:2294
msgid "HELP <command>, shows help for command."
msgstr "HELP <příkaz>, zobrazí nápovědu k příkazu."

#: src/irctabitem.cpp:2299
msgid "INVITE <nick> <channel>, invites someone to a channel."
msgstr "INVITE <přezdívka> <kanál>, pozve někoho na kanál."

#: src/irctabitem.cpp:2304
msgid "JOIN <channel>, joins a channel."
msgstr "JOIN <kanál>, připojí kanál."

#: src/irctabitem.cpp:2309
msgid "KICK <channel> <nick>, kicks a user from a channel."
msgstr "KICK <kanál> <přezdívka>, vykopne uživatele z kanálu."

#: src/irctabitem.cpp:2314
msgid "KILL <user> [reason], kills a user from the network."
msgstr "KILL <uživatel> [důvod], zabije uživatele."

#: src/irctabitem.cpp:2319
msgid "LIST [channel], lists channels and their topics."
msgstr "LIST [kanál], vypíše seznam kanálů a jejich témat."

#: src/irctabitem.cpp:2325
msgid "LUA help, shows help for lua scripting."
msgstr "LUA help, zobrazí nápovědu ke skriptování pomocí Lua."

#: src/irctabitem.cpp:2326
msgid "LUA load <path>, loads script."
msgstr "LUA load <cesta>, načte skript."

#: src/irctabitem.cpp:2327
msgid "Example: /lua load /home/dvx/test.lua"
msgstr "Příklad: /lua load /home/dvx/test.lua"

#: src/irctabitem.cpp:2328
msgid "LUA unload <name>, unloads script."
msgstr "LUA unload <jméno>, odebere skript."

#: src/irctabitem.cpp:2329
msgid "Example: /lua unload test"
msgstr "Příklad: /lua unload test"

#: src/irctabitem.cpp:2330
msgid "LUA list, shows list of loaded scripts"
msgstr "LUA list, zobrazí seznam načtených skriptů"

#: src/irctabitem.cpp:2342
msgid "ME <to> <message>, sends the action."
msgstr "ME <komu> <zprávu>, pošle akční zprávu."

